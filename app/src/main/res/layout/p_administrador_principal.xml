<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/txtPAdministradorPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Benvolgut Administrador"
        tools:layout_editor_absoluteX="125dp"
        tools:layout_editor_absoluteY="81dp" />

    <Button
        android:id="@+id/btnPAdministradorPrincipal_nouUsuari"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Afegir nou usuari"
        tools:layout_editor_absoluteX="134dp"
        tools:layout_editor_absoluteY="150dp" />

    <Button
        android:id="@+id/btnPAdministradorPrincipal_ElimiarModificarUsuari"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Modificar/Eliminar usuari"
        tools:layout_editor_absoluteX="76dp"
        tools:layout_editor_absoluteY="234dp" />

    <Button
        android:id="@+id/btnPAdministradorPrincipal_nouProducte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Afegir nou producte"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="341dp" />

    <Button
        android:id="@+id/btnPAdministradorPrincipal_AdministrarProductes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Administrar Productes"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="434dp" />
</androidx.constraintlayout.widget.ConstraintLayout>